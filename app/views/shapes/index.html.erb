<p id="notice"><%= notice %></p>

<h1>Shapes</h1>

<table>
  <thead>
    <tr>
      <th>Fsa</th>
      <th>Btm left x</th>
      <th>Btm left y</th>
      <th>Btm right x</th>
      <th>Btm right y</th>
      <th>Top right x</th>
      <th>Top right y</th>
      <th>Top left x</th>
      <th>Top left y</th>
      <th>Polygon</th>
      <th>Point</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shapes.each do |shape| %>
      <tr>
        <td><%= shape.fsa %></td>
        <td><%= shape.btm_left_x %></td>
        <td><%= shape.btm_left_y %></td>
        <td><%= shape.btm_right_x %></td>
        <td><%= shape.btm_right_y %></td>
        <td><%= shape.top_right_x %></td>
        <td><%= shape.top_right_y %></td>
        <td><%= shape.top_left_x %></td>
        <td><%= shape.top_left_y %></td>
        <td><%= shape.polygon %></td>
        <td><%= shape.point %></td>
        <td><%= link_to 'Show', shape %></td>
        <td><%= link_to 'Edit', edit_shape_path(shape) %></td>
        <td><%= link_to 'Destroy', shape, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shape', new_shape_path %>
